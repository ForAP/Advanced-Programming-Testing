<xml>
<turingmachine>
  <alphabet>01</alphabet>
  <initialtape>1010101</initialtape>
  <blank char="0"/>
  <initialstate name="0" />
  <finalstate name="halt" />
  <states>
    <state name="0">
      <transition seensym="0" writesym="0" newstate="1" move="LEFT"/>
      <transition seensym="1" writesym="0" newstate="0" move="RIGHT" />
    </state>
    <state name="1">
      <transition seensym="0" writesym="0" newstate="halt" move="LEFT"/>
      <transition seensym="1" writesym="0" newstate="halt" move="RIGHT" />
    </state>
  </states>
</turingmachine>
</xml>
